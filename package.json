{
  "name": "@foony/vite-plugin-import-map",
  "version": "1.0.0",
  "description": "Vite plugin that generates import maps to prevent cascading hash changes. Created by foony.com",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "development": "./src/index.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "import-map",
    "import-maps",
    "rollup",
    "build",
    "foony"
  ],
  "author": {
    "name": "Foony",
    "url": "https://foony.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Foony-Limited/vite-plugin-import-map.git"
  },
  "dependencies": {
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "magic-string": "^0.30.21"
  },
  "devDependencies": {
    "@types/node": "^22.13.0",
    "typescript": "^5.9.0"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "rollup": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    }
  }
}
